{
  "family": "gellc-prefect-task",
  "networkMode": "awsvpc",
  "taskRoleArn": "arn:aws:iam::576671272815:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::576671272815:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "gellc-prefect-container",
      "image": "576671272815.dkr.ecr.us-east-1.amazonaws.com/gellc-prefect:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PYTHONPATH",
          "value": "/app"
        },
        {
          "name": "PREFECT_MODE",
          "value": "worker"
        },
        {
          "name": "PREFECT_WORK_POOL",
          "value": "gellc-ecs-pool"
        },
        {
          "name": "PREFECT_API_URL",
          "value": "https://api.prefect.cloud/api/accounts/ab61b83d-af98-4940-ac58-024d88160a03/workspaces/e31cc9e9-de96-4558-acdc-1ded94493b8d"
        },
        {
          "name": "PREFECT_API_KEY",
          "value": "pnu_Mt1EbbVRvkQ8abHqFrhA2BRZJrsg803AcJOR"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/gellc-prefect-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
